    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-loader-spinner": "^3.1.14",
    "react-player": "^2.0.1",
    "react-pose": "^4.0.10",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
